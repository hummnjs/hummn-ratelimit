{
  "name": "@hummn/ratelimit",
  "module": "index.ts",
  "type": "module",
  "version": "0.2.0",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "bun run ./build/build.ts && tsc --emitDeclarationOnly --outDir dist",
    "prepublishOnly": "bun run build",
    "test": "bun test"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hummnjs/hummn-ratelimit"
  },
  "author": {
    "name": "Simeon Akpanudo",
    "url": "https://x.com/cmiondotdev"
  },
  "exports": {
    "./bun": {
      "types": "./dist/bun/index.d.ts",
      "import": "./dist/bun.js",
      "default": "./dist/bun.js"
    },
    "./node": {
      "types": "./dist/node.d.ts",
      "import": "./dist/node.js"
    },
    ".": {
      "types": "./dist/bun/index.d.ts",
      "import": "./dist/bun.js",
      "default": "./dist/bun.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "devDependencies": {
    "@biomejs/biome": "2.2.5",
    "@redis/client": "^5.8.3",
    "@types/bun": "latest",
    "@types/ms": "^2.1.0",
    "ultracite": "5.6.2"
  },
  "peerDependencies": {
    "@redis/client": "^5.0.0",
    "typescript": "^5"
  },
  "peerDependenciesMeta": {
    "@redis/client": {
      "optional": true
    }
  },
  "dependencies": {
    "ms": "^2.1.3"
  },
  "packageManager": "bun@1.3.1"
}
